{
  "name": "front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write . --config .prettierrc",
    "format:check": "prettier --check . --config .prettierrc",
    "format:staged": "prettier --write --config .prettierrc",
    "format:vue": "prettier --write \"src/**/*.vue\" --config .prettierrc",
    "format:ts": "prettier --write \"src/**/*.{ts,js}\" --config .prettierrc"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "apexcharts": "^4.5.0",
    "axios": "^1.8.1",
    "date-fns": "^4.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-apexcharts": "^1.8.0",
    "vuetify": "^3.7.14"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "prettier": "^3.6.2",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  }
}
